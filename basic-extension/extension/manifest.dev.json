{
  "manifest_version": 2,
  "name": "[DEVELOPMENT] Burner Extension",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "wallet.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "content-script.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "injected.bundle.js"
  ]
}
